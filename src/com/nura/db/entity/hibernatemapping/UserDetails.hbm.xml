<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.nura.db.entity">
  <class name="UserDetails" table="UserDetails">
    <id column="id" name="id" type="int">
      <generator class="native"/>
    </id>
    <property column="username" name="userName" type="string"/>
    <property column="password" name="pwd" type="string"/>
    <property column="mailid" name="emailid" type="string"/>
    <property column="address" name="address" type="string"/>
    <property column="phno" name="phoneNumber" type="string"/>
    <property column="privateKey" name="privateKey" type="string"/>
    <property column="publicKey" name="publicKey" type="string"/>
    <property column="groupId" name="groupId" type="string"/>
    
  </class>
</hibernate-mapping>
